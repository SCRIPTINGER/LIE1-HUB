--[[
   (Alpha 0.1.0) ~  WHITELIST

]]--
local whitelistecheck = loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTINGER/LIE1-HUB/refs/heads/main/Scripts/Hubs/Hub_3", true))()

if whitelistecheck[game:service('Players').LocalPlayer.UserId] then
  game:service('Players').LocalPlayer:Kick('you in the whitelist')
else
  print("loading....")
  loadstring(game:HttpGet("https://raw.githubusercontent.com/SCRIPTINGER/LIE1-HUB/refs/heads/main/Scripts/Hubs/Hub_1", true))()
end
