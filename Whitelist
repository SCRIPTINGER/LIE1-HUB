local whitelistecheck = loadstring(game:HttpGet('https://raw.githubusercontent.com/SCRIPTINGER/LIE1-HUB/refs/heads/main/Data-Base'))()

if whitelistecheck[game:service('Players').LocalPlayer.UserId] then
  loadstring(game:HttpGet('https://raw.githubusercontent.com/SCRIPTINGER/LIE1-HUB/refs/heads/main/Scripts/LIE1-HUB(ACTIVATE%20SYSTEM)'))()
else
  game:service('Players').LocalPlayer:Kick('Kid')
end
